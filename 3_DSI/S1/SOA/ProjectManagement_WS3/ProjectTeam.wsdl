<?xml version="1.0" encoding="utf-8"?>
<definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             xmlns:tns="http://example.com/projectteam"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
             targetNamespace="http://example.com/projectteam">

  <message name="AssignTeamToProjectRequest">
    <part name="ProjectId" type="xsd:int"/>
    <part name="TeamId" type="xsd:int"/>
  </message>

  <message name="AssignTeamToProjectResponse">
    <part name="status" type="xsd:string"/>
  </message>

  <portType name="ProjectTeamPortType">
    <operation name="AssignTeamToProject">
      <input message="tns:AssignTeamToProjectRequest"/>
      <output message="tns:AssignTeamToProjectResponse"/>
    </operation>
  </portType>

  <binding name="ProjectTeamBinding" type="tns:ProjectTeamPortType">
    <soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
    <operation name="AssignTeamToProject">
      <soap:operation soapAction="AssignTeamToProject"/>
      <input>
        <soap:body use="encoded" namespace="http://example.com/projectteam" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
      </input>
      <output>
        <soap:body use="encoded" namespace="http://example.com/projectteam" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
      </output>
    </operation>
  </binding>

  <service name="ProjectTeamService">
    <port name="ProjectTeamPort" binding="tns:ProjectTeamBinding">
      <soap:address location="http://localhost:8000/ProjectTeamService"/>
    </port>
  </service>

</definitions>
