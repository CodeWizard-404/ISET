
<!DOCTYPE Metadata [
  <!ENTITY % BIND_VALUES PUBLIC "OLAP BIND VALUES" "OLAP METADATA">
  %BIND_VALUES;
  ]>
<Metadata
  Version="1.1">
  <Cube
    Name="SALES_CUBE"
    ETViewName="SALES_CUBE_VIEW">
    <Organization>
      <AWCubeOrganization
        NullStorage="MV_READY"
        SparseType="COMPRESSED"
        CubeStorageType="NUMBER"
        PrecomputePercent="30"
        MVOption="NONE"
        MeasureStorage="SHARED">
        <AW
          Name="&AW_NAME;"/>
        <SparseDimension
          Name="CHANNEL"/>
        <SparseDimension
          Name="TIME"/>
        <SparseDimension
          Name="GEOGRAPHY"/>
        <SparseDimension
          Name="PRODUCT"/>
        <DefaultBuild>
          <![CDATA[BUILD SPEC Load_and_Aggregate
(
  LOAD NO SYNCH, 
  SOLVE
)]]>
        </DefaultBuild>
      </AWCubeOrganization>
    </Organization>
    <Dimensionality
      Name="CHANNEL"
      ETKeyColumnName="CHANNEL"
      ETAttrPrefix="D1_">
      <Dimension
        Name="CHANNEL"/>
    </Dimensionality>
    <Dimensionality
      Name="TIME"
      ETKeyColumnName="TIME"
      ETAttrPrefix="D2_">
      <Dimension
        Name="TIME"/>
    </Dimensionality>
    <Dimensionality
      Name="GEOGRAPHY"
      ETKeyColumnName="GEOGRAPHY"
      ETAttrPrefix="D3_">
      <Dimension
        Name="GEOGRAPHY"/>
    </Dimensionality>
    <Dimensionality
      Name="PRODUCT"
      ETKeyColumnName="PRODUCT"
      ETAttrPrefix="D4_">
      <Dimension
        Name="PRODUCT"/>
    </Dimensionality>
    <ConsistentSolve>
      <![CDATA[SOLVE 
(
  SUM OVER CHANNEL, 
  SUM OVER "TIME", 
  SUM OVER GEOGRAPHY, 
  SUM OVER PRODUCT
)]]>
    </ConsistentSolve>
    <Description
      Type="LongDescription"
      Language="AMERICAN"
      Value="Sales Cube">
    </Description>
    <Description
      Type="ShortDescription"
      Language="AMERICAN"
      Value="Sales Cube">
    </Description>
    <Description
      Type="Description"
      Language="AMERICAN"
      Value="Sales Cube">
    </Description>
    <Measure>
      <BaseMeasure
        Name="SALES"
        SQLDataType="NUMBER"
        ETMeasureColumnName="SALES">
        <Description
          Type="LongDescription"
          Language="AMERICAN"
          Value="Sales">
        </Description>
        <Description
          Type="ShortDescription"
          Language="AMERICAN"
          Value="Sales">
        </Description>
        <Description
          Type="Description"
          Language="AMERICAN"
          Value="Sales">
        </Description>
      </BaseMeasure>
    </Measure>
    <Measure>
      <BaseMeasure
        Name="QUANTITY"
        SQLDataType="NUMBER"
        ETMeasureColumnName="QUANTITY">
        <Description
          Type="LongDescription"
          Language="AMERICAN"
          Value="Quantity">
        </Description>
        <Description
          Type="ShortDescription"
          Language="AMERICAN"
          Value="Quantity">
        </Description>
        <Description
          Type="Description"
          Language="AMERICAN"
          Value="Quantity">
        </Description>
      </BaseMeasure>
    </Measure>
    <CubeMap
      Name="MAP1"
      Query="SALES_FACT">
      <CubeDimensionalityMap
        Name="CHANNEL"
        Expression="CHANNELS.CHANNEL_KEY"
        JoinCondition="SALES_FACT.CHANNEL = CHANNELS.CHANNEL_KEY">
        <Dimensionality
          Name="CHANNEL"/>
        <MappedDimension
          Name="CHANNEL.SALES_CHANNEL.CHANNEL"/>
      </CubeDimensionalityMap>
      <CubeDimensionalityMap
        Name="TIME"
        Expression="TIMES.MONTH_ID"
        JoinCondition="SALES_FACT.DAY_KEY = TIMES.DAY_KEY">
        <Dimensionality
          Name="TIME"/>
        <MappedDimension
          Name="TIME.CALENDAR.MONTH"/>
      </CubeDimensionalityMap>
      <CubeDimensionalityMap
        Name="GEOGRAPHY"
        Expression="CUSTOMERS.STATE_PROVINCE_KEY"
        JoinCondition="SALES_FACT.CUSTOMER = CUSTOMERS.CUSTOMER_KEY">
        <Dimensionality
          Name="GEOGRAPHY"/>
        <MappedDimension
          Name="GEOGRAPHY.REGIONAL.STATE_PROVINCE"/>
      </CubeDimensionalityMap>
      <CubeDimensionalityMap
        Name="PRODUCT"
        Expression="PRODUCTS.ITEM_KEY"
        JoinCondition="SALES_FACT.PRODUCT = PRODUCTS.ITEM_KEY">
        <Dimensionality
          Name="PRODUCT"/>
        <MappedDimension
          Name="PRODUCT.STANDARD.ITEM"/>
      </CubeDimensionalityMap>
      <MeasureMap
        Name="SALES"
        Expression="SALES_FACT.SALES">
        <Measure
          Name="SALES"/>
      </MeasureMap>
      <MeasureMap
        Name="QUANTITY"
        Expression="SALES_FACT.QUANTITY">
        <Measure
          Name="QUANTITY"/>
      </MeasureMap>
    </CubeMap>
  </Cube>
</Metadata>